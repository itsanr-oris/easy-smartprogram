{
    "name": "f-oris/easy-smartprogram",
    "description": "百度智能小程序SDK",
    "keywords": [
        "baidu",
        "smartprogram",
        "baidu-sdk",
        "smartprogram-sdk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "F.oris",
            "email": "us@f-oris.me"
        }
    ],
    "require": {
        "php": ">=7.1",
        "pimple/pimple": "^3.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.24",
        "illuminate/support": "^5.6",
        "symfony/cache": "^4.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "mockery/mockery": "^1.2",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "EasySmartProgram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EasySmartProgram\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
